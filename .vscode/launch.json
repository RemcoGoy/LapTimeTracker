  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "server: nuxt",
      "outputCapture": "std",
      "program": "${workspaceFolder}/frontend/node_modules/nuxi/bin/nuxi.mjs",
      "args": [
        "dev"
      ],
    },
    {
      "name": "Python: FastAPI",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "api.main:app",
        "--reload"
      ],
      "jinja": false,
      "justMyCode": false
    }
  ]
}